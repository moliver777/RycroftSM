<h2>Admin</h2>
<table id="admin_links">
	<tr>
		<% if current_user.user_level == User::MASTER %>
			<td link="/admin/users">Users</td>
			<td link="/admin/venues">Venues</td>
		<% end %>
		<td link="/stock">Stock</td>
	</tr>
	<% if current_user.user_level == User::MASTER %>
		<tr>
			<td colspan="3" link="/admin/settings">Preferences & Site Settings</td>
		</tr>
	<% end %>
</table>

<script type="text/javascript">
	$(document).ready(function() {
		$.each($("table#admin_links td"), function(i,link) {
			$(link).unbind("click").click(function() {
				window.location.href = $(this).attr("link");
			})
		})
	})
</script>