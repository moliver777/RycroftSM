<h2>Admin</h2>
<table id="admin_links">
	<tr>
		<td link="users">Users</td>
		<td link="venues">Venues</td>
	</tr>
	<tr>
		<td colspan="2" link="settings">Preferences & Site Settings</td>
	</tr>
</table>

<script type="text/javascript">
	$(document).ready(function() {
		$.each($("table#admin_links td"), function(i,link) {
			$(link).unbind("click").click(function() {
				window.location.href = "/admin/"+$(this).attr("link");
			})
		})
	})
</script>